{
  "schemaVersion": 1,
  "id": "terramath",
  "version": "${version}",

  "name": "TerraMath",
  "description": "Generate your Minecraft worlds using mathematical formulas! Create unique terrains from waves to spirals with custom equations.",
  "authors": ["addavriance"],
  "contact": {
    "homepage": "https://modrinth.com/mod/terra-math",
    "sources": "https://github.com/addavriance/TerraMath",
    "issues": "https://github.com/addavriance/TerraMath/issues"
  },

  "license": "GPL-3.0-only",
  "icon": "icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.adda.terramath.fabric.TerraMathFabric"
    ]
  },
  "mixins": [
    "terramath.mixins.json",
    "terramath-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.9",
    "fabric-api": ">=0.97.5",
    "minecraft": ">=1.20.5 <=1.21.4"
  },
  "suggests": {
    "modmenu": ">=9.0.0"
  },
  "custom": {
    "modmenu": {
      "update_checker": true,
      "links": {
        "modmenu.modrinth": "https://modrinth.com/mod/terra-math",
        "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/terra-math",
        "modmenu.issues": "https://github.com/addavriance/TerraMath/issues"
      }
    }
  }
}
